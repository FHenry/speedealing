{
	"_id": "extrafields:Societe",
	"class": "extrafields",
	"ico": "icon-object-company",
	"schemaMongoose": {
		"enabled": true,
		"name": "societe",
		"collection": "Societe",
		"plugins": ["mongoose-timestamp"]
	},
	"langs": [
		"companies",
		"bills",
		"compta"
	],
	"pluginsList": [
		"RowAction",
		"TrashNotification",
		"RowShowHide",
		"FieldEditable",
		"ColFilter"
	],
	"createList": [
		"name",
		"Status",
		"commercial_id",
		"address",
		"zip",
		"town"
	],
	"oldList": [
		"code_client",
		"zip",
		"town",
		"address",
		"zonegeo",
		"phone",
		"idprof3"
	],
	"longList": [
		"_id",
		"Checkbox",
		"name",
		"code_client",
		"commercial_id",
		"zip",
		"town",
		"address",
		"zonegeo",
		"phone",
		"email",
		"idprof3",
		"Tag",
		"Status",
		"prospectlevel",
		"Action"
	],
	"shortList": [
		"town",
		"phone"
	],
	"fields": {
		"_id": {
			"schema": "ObjectId",
			"type": "text",
			"enable": true,
			"list": {
				"visible": false,
				"static": true
			}
		},
		"ref": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"name": {
			"schema": {"type": "String", "require": true},
			"type": "text",
			"label": "Company",
			"length": 60,
			"enable": true,
			"list": {
				"static": true
			},
			"render": {
				"type": "url",
				"ico": "icon-object-company"
			},
			"validate": {
				"cssclass": "required",
				"minlength": 3
			}
		},
		"prefix": {
			"type": "text",
			"length": 5,
			"enable": false,
			"edit": true
		},
		"code_client": {
			"schema": "String",
			"type": "text",
			"label": "CustomerCode",
			"length": 15,
			"enable": true,
			"list": {
				"cssclass": "center",
				"visible": false
			}
		},
		"code_fournisseur": {
			"schema": "String",
			"type": "text",
			"length": 15,
			"enable": false
		},
		"barcode": {
			"type": "text",
			"enable": false
		},
		"Status": {
			"schema": {"type": "Mixed", "default": "ST_NEVER"},
			"type": "select",
			"label": "Status",
			"enable": true,
			"default": "ST_NEVER",
			"status": true,
			"dict": "dict:fk_stcomm",
			"list": {
				"cssclass": "center",
				"editable": true
			},
			"render": {
				"type": "status"
			}
		},
		"address": {
			"schema": "String",
			"type": "textarea",
			"label": "Address",
			"cols": 40,
			"rows": 3,
			"enable": true,
			"list": {
				"visible": false
			}
		},
		"zip": {
			"schema": "String",
			"type": "text",
			"label": "Zip",
			"length": 15,
			"enable": true,
			"javascript": null,
			"list": {
				"cssclass": "center dol_edit",
				"editable": true
			}
		},
		"town": {
			"schema": "String",
			"type": "text",
			"label": "Town",
			"length": 15,
			"enable": true,
			"javascript": null,
			"list": {
				"cssclass": "dol_edit",
				"editable": true
			}
		},
		"country_id": {
			"schema": {"type": "String", "default": "FR"},
			"type": "select",
			"enable": true,
			"dict": "dict:fk_country",
			"default": "substr($langs->defaultlang,3,5)",
			"sort": true
		},
		"state_id": {
			"schema": "Number",
			"type": "select",
			"enable": true,
			"settype": "int",
			"dict": "dict:fk_departements",
			"filters": {
				"pays_code": "country_id"
			},
			"sort": true
		},
		"DefaultLang": {
			"type": "select",
			"enable": false,
			"dict": null
		},
		"logo": {
			"type": "image",
			"width": "130px",
			"alt": "Logo",
			"enable": true
		},
		"phone": {
			"schema": "String",
			"type": "text",
			"ico": "AC_TEL",
			"label": "Phone",
			"enable": true,
			"list": {
				"cssclass": "center",
				"editable": true,
				"visible": false
			},
			"mask": "+99 9 99 99 99 99"
		},
		"fax": {
			"schema": "String",
			"type": "text",
			"label": "Fax",
			"ico": "AC_FAX",
			"enable": true,
			"mask": "+99 9 99 99 99 99"
		},
		"email": {
			"schema": "String",
			"type": "email",
			"ico": "AC_EMAIL",
			"enable": true,
			"label": "EMail",
			"validate": {
				"cssclass": "email"
			},
			"list": {
				"cssclass": "center",
				"editable": true,
				"visible": false
			},
			"render": {
				"type": "email"
			}
		},
		"url": {
			"schema": "String",
			"type": "text",
			"ico": "AC_URL",
			"site": "www",
			"enable": true,
			"validate": {
				"cssclass": "url"
			}
		},
		"idprof1": {
			"type": "text",
			"label": "ProfId1",
			"length": "25",
			"enable": true,
			"list": {
				"visible": false
			}
		},
		"idprof3": {
			"type": "text",
			"label": "ProfId3FR",
			"length": "12",
			"enable": true,
			"list": {
			}
		},
		"typent_id": {
			"schema": {"type": "String", "default": "TE_UNKNOWN"},
			"type": "select",
			"default": "TE_UNKNOWN",
			"enable": true,
			"dict": "dict:fk_typent"
		},
		"effectif_id": {
			"schema": {"type": "String", "default": "EF0"},
			"type": "select",
			"enable": true,
			"default": "EF0",
			"dict": "dict:fk_effectif"
		},
		"capital": {
			"schema": {"type": "Number", "default": 0},
			"type": "text",
			"settype": "int",
			"length": "20",
			"label": "Capital",
			"enable": true,
			"spinner": "min: 0"
		},
		"VATIsUsed": {
			"schema": {"type": "Boolean", "default": true},
			"type": "checkbox",
			"settype": "bool",
			"enable": true,
			"default": true
		},
		"VATIntra": {
			"type": "text",
			"length": 25,
			"javascript": null,
			"enable": true
		},
		"forme_juridique_code": {
			"schema": "String",
			"type": "select",
			"label": "JuridicalStatus",
			"dict": "dict:fk_forme_juridique",
			"enable": true
		},
		"commercial_id": {
			"schema": {
				"id": {"type": "String"},
				"name": "String"
			},
			"type": "select",
			"label": "SalesRepresentatives",
			"class": "User",
			"mongo": {
				"collection": "User",
				"method": "find",
				"query": {
					"Status": "ENABLE"
				},
				"order": {
					"name": 1
				}
			},
			"enable": true,
			"list": {
				"editable": true
			},
			"render": {
				"type": "url",
				"ico": "icon-object-user"
			}
		},
		"cptBilling": {
			"schema": {
				"id": {"type": "ObjectId"},
				"name": "String"
			},
			"type": "select",
			"label": "Compte de facturation",
			"class": "Societe",
			"mongo": {
				"collection": "Societe",
				"method": "find",
				"query": {
					"Status": {"$nin": ["ST_NEVER","ST_NO"]}
				},
				"order": {
					"name": 1
				}
			},
			"enable": true,
			"list": {
				"editable": true
			},
			"render": {
				"type": "url",
				"ico": "icon-object-company"
			}
		},
		"civilite_id": {
			"schema": {"type": "String", "default": "NO"},
			"type": "select",
			"label": "UserTitle",
			"default": "NO",
			"dict": "dict:fk_civilite",
			"enable": true
		},
		"price_level": {
			"schema": {"type": "String", "default": "base"},
			"type": "select",
			"enable": true,
			"settype": "string",
			"default": "base",
			"label": "PriceLevel",
			"class": "Product",
			"view": "listPriceLevel",
			"getkey": true,
			"params": {
				"group": true
			}
		},
		"prospectlevel": {
			"schema": {"type": "String", "default": "PL_NONE"},
			"type": "select",
			"label": "ProspectLevelShort",
			"enable": true,
			"default": "PL_NONE",
			"status": true,
			"dict": "dict:fk_prospectlevel",
			"list": {
				"cssclass": "center",
				"editable": true
			},
			"render": {
				"type": "status"
			}
		},
		"cond_reglement": {
			"schema": "String",
			"type": "select",
			"label": "PaymentConditions",
			"dict": "dict:fk_payment_term",
			"enable": true
		},
		"mode_reglement": {
			"schema": "String",
			"type": "select",
			"label": "PaymentMode",
			"dict": "dict:fk_paiement",
			"enable": true
		},
		"currency": {
			"type": "select",
			"label": "CompanyCurrency",
			"dict": "dict:fk_currencies",
			"enable": true
		},
		"zonegeo": {
			"schema": "String",
			"type": "select",
			"label": "ZoneGeo",
			"enable": true,
			"settype": "string",
			"list": {
				"cssclass": "center",
				"editable": true
			},
			"view": "listZoneGeo",
			"getkey": true,
			"params": {
				"group": true
			}
		},
		"Tag": {
			"schema": ["String"],
			"type": "tag",
			"label": "Categories",
			"enable": true,
			"isarray": true,
			"list": {
			},
			"render": {
				"cssclass": "tag anthracite-gradient glossy"
			}
		},
		"notes": {
			"schema": "String",
			"type": "textarea",
			"enable": true
		},
		"public_notes": {
			"schema": "String",
			"type": "textarea",
			"enable": true
		},
		"code_compta": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"code_compta_fournisseur": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"user_creat": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"user_modif": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"remise_client": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"entity": {
			"schema": "String",
			"type": "text",
			"enable": true
		},
		"fournisseur": {
			"schema": {"type": "String", "default": "NO"},
			"type": "select",
			"enable": true,
			"label": "Fournisseur",
			"status": true,
			"values": {
				"NO": {
					"enable": true,
					"label": "Client seulement",
					"cssClass": "anthracite-gradient"
				},
				"SUPPLIER": {
					"enable": true,
					"label": "Fournisseur",
					"cssClass": "anthracite-gradient"
				},
				"SUBCONTRACTOR": {
					"enable": true,
					"label": "Sous-traitant",
					"cssClass": "anthracite-gradient"
				}
			}
		},
		"gps": {
			"schema": ["Number"],
			"type": "text",
			"enable": true
		},
		"contractID": {
			"optional": true,
			"schema": "String",
			"label": "Numero de contrat UGAP",
			"type": "text",
			"enable": true
		},
		"UGAP_Ref_Client": {
			"optional": true,
			"schema": "String",
			"label": "Numero de client UGAP",
			"type": "text",
			"enable": true
		},
		"Action": {
			"type": "action",
			"enable": true,
			"list": {
				"width": 45,
				"cssclass": "center",
				"sortable": false,
				"searchable": false,
				"static": true
			},
			"action": {
				"edit": {
					"label": "Edit",
					"cssclass": "icon-action-edit action-edit"
				}
			}
		},
		"Checkbox": {
			"type": "checkbox",
			"label": false,
			"enable": true,
			"list": {
				"width": 8,
				"sortable": false,
				"searchable": false,
				"static": true
			},
			"render": {
			}
		},
		"datec": {
			"schema": "Date"
		}
	}
}